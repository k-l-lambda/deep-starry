    "import cv2\n",
    "import numpy as np\n",
    "from starry.vision.layout_predictor import PageLayout\n",
    "\n",
    "\n",
    "hotmap = cv2.imread('./temp/test.png')\n",
    "hotmap = np.moveaxis(hotmap, -1, 0)\n",
    "hotmap = hotmap[::-1, :, :]\n",
    "print('hotmap:', hotmap.shape)\n",
    "layout = PageLayout(hotmap)\n",
    "print('layout:', layout.json())\n"
